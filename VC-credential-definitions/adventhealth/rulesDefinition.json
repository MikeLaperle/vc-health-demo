{
  "attestations": {
    "presentations": [
      {
        "credentialType": "UnitedHealthEmployeeCredential",
        "required": true,
        "mapping": [
          { "outputClaim": "employeeId", "inputClaim": "$.vc.credentialSubject.employeeId" },
          { "outputClaim": "department", "inputClaim": "$.vc.credentialSubject.department" }
        ]
      },
      {
        "credentialType": "FloridaMedicalLicenseCredential",
        "required": true,
        "mapping": [
          { "outputClaim": "licenseNumber", "inputClaim": "$.vc.credentialSubject.licenseNumber" },
          { "outputClaim": "licenseType", "inputClaim": "$.vc.credentialSubject.licenseType" },
          { "outputClaim": "licenseExpiration", "inputClaim": "$.vc.credentialSubject.licenseExpiration" }
        ]
      },
      {
        "credentialType": "MedicalDegreeCredential",
        "required": true,
        "mapping": [
          { "outputClaim": "degreeType", "inputClaim": "$.vc.credentialSubject.degreeType" },
          { "outputClaim": "fieldOfStudy", "inputClaim": "$.vc.credentialSubject.fieldOfStudy" },
          { "outputClaim": "graduationYear", "inputClaim": "$.vc.credentialSubject.graduationYear" },
          { "outputClaim": "institution", "inputClaim": "$.vc.credentialSubject.institution" }
        ]
      },
      {
        "credentialType": "AMACredential",
        "required": true,
        "mapping": [
          { "outputClaim": "amaId", "inputClaim": "$.vc.credentialSubject.amaId" },
          { "outputClaim": "membershipStatus", "inputClaim": "$.vc.credentialSubject.membershipStatus" },
          { "outputClaim": "membershipLevel", "inputClaim": "$.vc.credentialSubject.membershipLevel" }
        ]
      }
